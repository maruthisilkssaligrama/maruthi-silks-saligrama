{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "maruthi-silks-saligrama.vercel.app" }
      ],
      "destination": "https://www.maruthisilkssaligrama.com/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
